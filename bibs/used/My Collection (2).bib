journal = {Journal of Informetrics},
keywords = {Altmetrics,Broader impact,Scientometrics,Societal impact},
number = {4},
pages = {1--24},
title = {{Do altmetrics point to the broader impact of research? An overview of benef...: .}},
url = {https://eds.a.ebscohost.com/eds/detail/detail?vid=2&sid=5861687e-54ef-466c-9507-53c2c7650b43@sessionmgr4001&hid=4105&bdata=Jmxhbmc9c3Ymc2l0ZT1lZHMtbGl2ZQ==#AN=edsrep.v8y2014i4p895.903&db=edsrep},
volume = {8},

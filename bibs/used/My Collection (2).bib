file = {:C\:/Users/tesse/Desktop/Files/Dropbox/BIBrep/Articles/Artificial Intelligence/Neural Networks/2016 artiﬁcial neural networks architectures for stock price prediction - comparisons - persio et al.pdf:pdf},
issn = {19984464},
journal = {International Journal of Circuits, Systems and Signal Processing},

file = {:C\:/Users/tesse/Desktop/Files/Dropbox/BIBrep/Thesis/envelope/envelope/2018 Modeling and FPGA-based implementation of an efficient and simple envelope detector using a Hilbert Transform FIR filter for ultrasound imaging applications - Assef.pdf:pdf},

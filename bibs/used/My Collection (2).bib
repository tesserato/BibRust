file = {:C\:/Users/tesse/Desktop/Files/Dropbox/BIBrep/Articles/Artificial Intelligence/Neural Networks/Sound/2019 Few-Shot Adversarial Learning of Realistic Neural Talking Head Models - Zakharov et al..pdf:pdf},

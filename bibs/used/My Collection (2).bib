file = {:C\:/Users/tesse/Desktop/Files/Dropbox/BIBrep/0 New PAPERS/2014 Experimental simulation of closed timelike curves - Ringbauer et al.pdf:pdf},
issn = {20411723},
journal = {Nature Communications},
number = {May},
pages = {1--7},
publisher = {Nature Publishing Group},
title = {{Experimental simulation of closed timelike curves}},
url = {http://dx.doi.org/10.1038/ncomms5145},
volume = {5},
year = {2014}
}

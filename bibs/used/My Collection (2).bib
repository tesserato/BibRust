keywords = {Amplitude modulation,Envelope,Inferior colliculus,Reverberation,Single-unit,Unanesthetized rabbit},
number = {10},
pages = {4452--4468},
pmid = {25762687},
title = {{Neural coding of sound envelope in reverberant environments}},
volume = {35},
year = {2015}

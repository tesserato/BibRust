file = {:C\:/Users/tesse/Desktop/Files/Dropbox/BIBrep/Articles/Artificial Intelligence/Neural Networks/2016 deep convolutional neural networks for computer-aided detection - cnn - shin et al.pdf:pdf},
issn = {1558254X},

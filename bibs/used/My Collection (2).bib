file = {:C\:/Users/tesse/Desktop/Files/Dropbox/BIBrep/0 New BOOKS/2010 NIST handbook of mathematical functions - Olver and National Institute of Standards and Technology (U.S.).pdf:pdf},
isbn = {9780521140638},
number = {Surf Iii},
pages = {968 p.},
title = {{NIST Handbook of Mathematical Functions Rainbow over Woolsthorpe Manor}},
url = {http://www.cambridge.org/us/knowledge/isbn/item6005311/?site_locale=en_US%5Cnhttp://dlmf.nist.gov},

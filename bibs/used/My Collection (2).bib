keywords = {Clustering,Data visualization,Feature map,Incremental learning,Pattern classification,Radial basis function,Self-organization,Two-spiral problem},
number = {9},
pages = {1441--1460},
title = {{Growing cell structures-A self-organizing network for unsupervised and supervised learning}},
volume = {7},
year = {1994}

keywords = {Autoregressive (AR) modeling,Discrete cosine transform (DCT),Discrete trigonometric transform (DTT),Frequency-domain linear prediction,Generalized discrete Fourier transform (GDFT),Hilbert envelope,Linear prediction,Linear prediction in spectral domain,Temporal noise shaping (TNS)},
number = {7},

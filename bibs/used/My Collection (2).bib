file = {:C\:/Users/tesse/Desktop/Files/Dropbox/BIBrep/0 New PAPERS/2019 Learning data-driven discretizations for partial differential equations - Bar-Sinai et al.pdf:pdf},
issn = {10916490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Coarse graining,Computational physics,Machine learning},
number = {31},

@article{Kusuma1992,
abstract = {The neural network paradigm has been gaining recognition in geophysics as an alternative approach to solving various problems, especially those associated with pattern recognition and prediction tasks. Most of the work that has been done uses some form of back-propagation algorithm. A back-propagation network is simple and easy to implement. However, it learns too slowly for many real-life, non-trivial applications and lends to scale up poorly as the problem gels larger and more complex. It is also prone to getting trapped in local minima on the error surface. This sludy evaluates the performance of the cascade-correlation learning architecture in first break picking and automatic trace editing tasks. Empirical results show that, in comparison to the back-propagation mcthod, cascade correlation learning enables us to achieve a 10 to 50-foldspcedup in both tasks without getting trapped in local minima. Since cascade-correlation is a dynamic learning process, the size of the net obtained aller the learning session is guaranteed to be nearly optimal. Unlike back-propagation, a cascade-correlation net can be trained Incrementally while retaining most of the knowledge from its previous training session. Therefore, a large training data set can be broken up into smaller pieces and training can be cumulative.},
author = {Kusuma, Tony and Brown, Mark M.},
doi = {10.1190/1.1821929},

doi = {10.1109/JSTQE.2019.2945540},
eprint = {1907.01525},
file = {:C\:/Users/tesse/Desktop/Files/Dropbox/BIBrep/Articles/Artificial Intelligence/Neural Networks/2019 Digital Electronics and Analog Photonics for Convolutional Neural Networks (DEAP-CNNs).pdf:pdf},

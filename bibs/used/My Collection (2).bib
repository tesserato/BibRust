file = {:C\:/Users/tesse/Desktop/Files/Dropbox/BIBrep/Articles/Artificial Intelligence/Neural Networks/Sound/Neural Machine Translation in Linear Time - Kalchbrenner et al..pdf:pdf},
title = {{Neural Machine Translation in Linear Time}},
url = {http://arxiv.org/abs/1610.10099},
year = {2016}
}

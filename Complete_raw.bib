volume = {7},
isbn = {1866-9956},
year = {2015},
journal = {Cognitive Computation},
keywords = {ELM,Extreme learning machine,Feedforward neural network,QuickNet,Radial basis function network,Random vector functional link,Randomness},
number = {3},
keywords = {elm},
mendeley-tags = {elm},
}

@book{2009BilbaoNumericala,
author = {Stefan Bilbao},
publisher = {John Wiley \& Sons, Ltd},
url = {https://www.ebook.de/de/product/9338750/stefan///////%5C%5C_bilbao///////%5C%5C_numerical///////%5C%5C_sound///////%5C%5C_synthesis.html},
isbn = {9780470510469},
month = {oct},

pages = {99--127},
pmid = {12180173},
publisher = {MIT Press},
title = {Evolving Neural Networks through Augmenting Topologies},
url = {http://www.mitpressjournals.org/doi/10.1162/106365602320169811},
volume = {10},
year = {2002},

keywords = {agile methods; software development; XP; Scrum; me},
month = {apr},
number = {2},
pages = {200--213},
publisher = {PALGRAVE MACMILLAN LTD},
title = {Customising agile methods to software practices at Intel Shannon},
type = {Article},
volume = {15},
year = {2006},
}


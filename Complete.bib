issn = {15577368},
journal = {ACM Transactions on Graphics},
keywords = {Motion control,Physics-based character animation,Reinforcement learning},
number = {4},
title = {DeepMimic: Example-guided deep reinforcement learning of physics-based character skills},
volume = {37},
year = {2018},
file = {C\:/Users/tesse/Desktop/Files/Dropbox/BIBrep/Articles/Artificial Intelligence/Reinforcement Learning/2018 deepmimic - example-guided deep reinforcement learning of physics-based - peng et al.pdf},

arxivid = {1706.02515v5},
eprint = {1706.02515v5},
issn = {10495258},
journal = {Advances in Neural Information Processing Systems},
pages = {972--981},
publisher = {Curran Associates, Inc.},
title = {Self-normalizing neural networks},
url = {http://papers.nips.cc/paper/6698-self-normalizing-neural-networks.pdf},
volume = {2017-Decem},
year = {2017},
file = {C\:/Users/tesse/Desktop/Files/Dropbox/BIBrep/Articles/Artificial Intelligence/Neural Networks/2017KlambauerSelf Self Normalizing Neural Networks.pdf},

isbn = {216223882162237},
issn = {21622388},
journal = {IEEE Transactions on Neural Networks and Learning Systems},
keywords = {Deep learning (DL),Deep neural network (DNN),ELM,Extreme learning machine (ELM),Multilayer perceptron (MLP),elm,random feature mapping},
number = {4},
pages = {1--13},
pmid = {25966483},
title = {Extreme Learning Machine for Multilayer Perceptron},
volume = {27},
year = {2016},
mendeley-tags = {#corrupted author,elm},

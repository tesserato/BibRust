journal = {2016 IEEE Winter Conference on Applications of Computer Vision, WACV 2016},
keywords = {image classification,optimization,theory},
pages = {1--8},
title = {On the importance of normalisation layers in deep learning with piecewise linear activation units},
year = {2016},
file = {C\:/Users/tesse/Desktop/Files/Dropbox/BIBrep/Articles/Artificial Intelligence/Neural Networks/Deep Learning/2016 On the importance of normalisation layers in deep learning with piecewise linear activation units - Liao and Carneiro.pdf},
mendeley-tags = {optimization,theory,image classification},

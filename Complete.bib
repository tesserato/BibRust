issn = {14606925},
journal = {Design Journal},
keywords = {cosmetic obsolescence,degradation,graceful ageing,material change,patina},
number = {sup1},
pages = {S160----S171},
title = {Design for Next\ldotsYear. The Challenge of Designing for Material Change},
volume = {20},
